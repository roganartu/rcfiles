{
    "suggest.autoTrigger": "always",
    "suggest.minTriggerInputLength": 2,
    "suggest.enablePreselect": true,
    "suggest.noselect": false,
    "suggest.timeout": 500,

    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootpatterns": ["go.mod"],
            "filetypes": ["go"]
        }
    },

    "python.pythonPath": "~/.venvs/vim/bin/python",
    "python.venvPath": "~/.venvs"
}
